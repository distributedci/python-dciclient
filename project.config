[project]
	description = A python client for DCI
[access "refs/*"]
  read = group dci-core
  owner = group dci-ptl
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group dci-core
	label-Verified = -2..+2 group dci-ptl
	label-Workflow = -1..+1 group dci-core
	submit = group dci-ptl
	read = group Registered Users
    read = group dci-core
	pushTag = group dci-core
	pushTag = group dci-ptl
	pushSignedTag = group dci-core
	pushSignedTag = group dci-ptl
[access "refs/meta/config"]
    read = group Registered Users
    read = group dci-core
[receive]
	requireChangeId = true
[submit]
	mergeContent = false
