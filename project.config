[project]
	description = A python client for DCI
[access "refs/*"]
  read = group python-dciclient-core
  owner = group python-dciclient-ptl
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group python-dciclient-core
	label-Verified = -2..+2 group python-dciclient-ptl
	label-Workflow = -1..+1 group python-dciclient-core
	submit = group python-dciclient-ptl
	read = group python-dciclient-core
    read = group Registered Users
[access "refs/meta/config"]
    read = group python-dciclient-core
    read = group Registered Users
[receive]
	requireChangeId = true
[submit]
	mergeContent = false
	action = fast forward only
